### CharOrDefault boundary cases
positive: CharOrDefault_boundary_conditions
	MODULE Test;
	IMPORT Strings;
	VAR s := "test": ARRAY 5 OF CHAR;
	BEGIN
		ASSERT(Strings.CharOrDefault(s, 0, 5) = 't');
		ASSERT(Strings.CharOrDefault(s, 3, 5) = 't');
		ASSERT(Strings.CharOrDefault(s, 4, 5) = 0X);
		ASSERT(Strings.CharOrDefault(s, 5, 5) = 0X);
		ASSERT(Strings.CharOrDefault(s, -1, 5) = 0X);
		ASSERT(Strings.CharOrDefault(s, 2, 5, 'X') = 's');
		ASSERT(Strings.CharOrDefault(s, 10, 5, 'X') = 'X');
	END Test.

### IsDigit, IsDecimal, IsHexadecimal
positive: IsDigit_boundary_values
	MODULE Test;
	IMPORT Strings;
	VAR d: SIGNED32;
	BEGIN
		ASSERT(Strings.IsDigit('0', 2, d) & (d = 0));
		ASSERT(Strings.IsDigit('1', 2, d) & (d = 1));
		ASSERT(~Strings.IsDigit('2', 2, d));
		ASSERT(Strings.IsDigit('9', 10, d) & (d = 9));
		ASSERT(~Strings.IsDigit('A', 10, d));
		ASSERT(Strings.IsDigit('A', 11, d) & (d = 10));
		ASSERT(Strings.IsDigit('Z', 36, d) & (d = 35));
		ASSERT(~Strings.IsDigit('a', 10, d));
		ASSERT(Strings.IsDigit('a', 11, d) & (d = 10));
		ASSERT(~Strings.IsDigit('@', 16, d));
		ASSERT(~Strings.IsDigit('[', 36, d));
		ASSERT(~Strings.IsDigit(0X, 10, d));
	END Test.

