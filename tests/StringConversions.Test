### Final comprehensive test combining multiple functions

positive: comprehensive_parsing_scenario
	MODULE Test;
	IMPORT Strings;
	VAR
		u32: UNSIGNED32; u64: UNSIGNED64;
		i32: SIGNED32; i64: SIGNED64;
		sz: SIZE; addr: ADDRESS;
		pos: SIZE; res: INTEGER;
	BEGIN
		(* Test 1: UNSIGNED32 with various bases *)
		ASSERT(Strings.TryParseUInt32("0b1101", u32) & (u32 = 13));
		ASSERT(Strings.TryParseUInt32("0o777", u32) & (u32 = 511));
		ASSERT(Strings.TryParseUInt32("1234", u32) & (u32 = 1234));
		ASSERT(Strings.TryParseUInt32("0xABCD", u32) & (u32 = 0xABCD));

		(* Test 2: SIGNED32 with signs and overflow *)
		ASSERT(Strings.TryParseInt32("-1234", i32) & (i32 = -1234));
		ASSERT(Strings.TryParseInt32("+1234", i32) & (i32 = 1234));
		ASSERT(~Strings.TryParseInt32("2147483648", i32) & (i32 = MAX(SIGNED32)));

		(* Test 3: UNSIGNED64 with separators using TryScan *)
		pos := 0;
		ASSERT(Strings.TryScanUInt64At(pos, "1'000'000'000", 00, u64, res) & (u64 = 1000000000) & (res = Strings.Ok));

		(* Test 4: SIGNED64 with hex *)
		ASSERT(Strings.TryParseInt64("-0x7FFFFFFFFFFFFFFF", i64) & (i64 = -0x7FFFFFFFFFFFFFFF));

		(* Test 5: SIZE (platform dependent) *)
		ASSERT(Strings.TryParseSize("4096", sz) & (sz = 4096));

		(* Test 6: ADDRESS *)
		ASSERT(Strings.TryParseAddress("0x1000", addr) & (addr = 0x1000));

		(* Test 7: Helper functions *)
		VAR d: SIGNED32;
		ASSERT(Strings.IsDecimal('7', d) & (d = 7));
		ASSERT(Strings.IsHexadecimal('E', d) & (d = 14));
		ASSERT(Strings.IsDigit('Z', 36, d) & (d = 35));

		(* Test 8: CharOrDefault *)
		VAR str := "Hello": ARRAY 6 OF CHAR;
		ASSERT(Strings.CharOrDefault(str, 1, 6) = 'e');
		ASSERT(Strings.CharOrDefault(str, 10, 6, '!') = '!');
	END Test.