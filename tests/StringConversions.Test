# StringConversions.Test
# options --mayTrap --prolog="Compiler.Compile TesterInput.txt" --command="System.Free Test B A;System.Load Test" --logFile="StringConversions.Test.Log" --result="StringConversions.Test.Diff"


# TryScanUInt64At

positive: TryScanUInt64At "", base = 0
	MODULE Test;
	IMPORT Strings;
	VAR value: UNSIGNED64; i := 0: SIZE; res: INTEGER;
	BEGIN
		ASSERT( ~Strings.TryScanUInt64At(i, "",  00, value, res) & (res = Strings.EmptyStringError));
	END Test.

